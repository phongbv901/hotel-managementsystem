    <roleManager enabled="true" defaultProvider="HoteRoleProvider">
      <providers>
        <clear />
        <add name="HoteRoleProvider" type="System.Web.Security.SqlRoleProvider"
             connectionStringName="HotelDBConnectionstring" applicationName="/" />
      </providers>
    </roleManager>


    <pages>
